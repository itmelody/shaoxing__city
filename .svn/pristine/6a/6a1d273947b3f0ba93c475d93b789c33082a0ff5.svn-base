.center {
  position: relative;
  height: 100%;
}

.overlay {
  width: 100%;
  height: 110px;
  //background-color: #4297ec;
  background: url("../../assets/half-round.png") no-repeat;
  background-size: 100% 100%;
  position: absolute;
}

.header {
  position: relative;
  color: #ffffff;
  margin-bottom: 20px;
  .title {
    font-size: 18px;
    letter-spacing: 1px;
    color: #ffffff;
  }
}

.card {
  margin: 20px;
  padding: 30px 0;
  border-radius: 10px;
  background-color: #ffffff;
  background-size: 100% 100%;
  position: relative;
  display: flex;
  flex-direction: column;
  align-items: center;
  justify-content: center;
  .register {
    position: absolute;
    top: 10px;
    right: 10px;
    font-size: 14px;
    color: #367be9;
    padding: 1px 5px;
    border: 1px solid #367be9;
    border-radius: 20px;
  }
  .logo {
    margin-bottom: 9px;
  }
  .tips {
    text-align: center;
    font-size: 16px;
    color: #4d4d4d;
    z-index: 1;
    .underline {
      color: #306ce7;
      text-decoration: underline;
    }
  }
  .userInfo {
    position: absolute;
    width: 100%;
    left: 0;
    right: 0;
    bottom: 0;
  }
}

.cell-icon {
  margin-right: 10px;
  padding: 0 5px;
  color: #fff;
  border-radius: 5px;
}

.logout {
  width: 90%;
  margin: 0 auto;
  margin-top: 40px;
  button {
    width: 100%;
  }
}
